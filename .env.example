# Copy this file to .env and fill in your values

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=project 
GOOGLE_CLOUD_LOCATION=region
GOOGLE_CLOUD_STORAGE=gs://storage_name 

# Vertex AI Configuration
VERTEX_AI_LOCATION=region
VERTEX_AGENT_RESOURCE_NAME=projects/test-disco-cm/locations/europe-west1/reasoningEngines/your-agent-id

# Gemini Model Configuration
GOOGLE_GENAI_USE_VERTEXAI=true
MODEL_NAME=gemini-2.5-flash

# Neo4j Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password-here
NEO4J_DATABASE=neo4j

# Authentication - Choose one:
# Option 1: Service Account Key
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Option 2: Use gcloud auth application-default login
# (No environment variable needed)

# Deployment Configuration
DEPLOYMENT_NAME=neo4j-agent-deployment
AGENT_NAME=neo4j-database-agent

# Logging and Monitoring
ENABLE_LOGGING=true
ENABLE_MONITORING=true
ENABLE_AUTO_SCALING=true

# Development Settings
DEBUG=false
LOG_LEVEL=INFO